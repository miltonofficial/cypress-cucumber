pipeline {
    agent any 
    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/miltonofficial/cypress-cucumber.git'
            }
        }
        stage('Install or Update Node Packages') {
            step{
             sh '''npm install'''
            }
        }     
        stage('Execute Tests') {
            step{
                sh '''npm test'''
            } 
        }      
    }
}